  <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=project.menu.id">
         <menu
               id="de.urszeidler.shr5.ecp.menus.characterMenu"
               label="%character.menu.label">
            <command
                  commandId="de.urszeidler.shr5.ecp.commands.openCharacterWizardCommand"
                  mnemonic="S">
            </command>
            <separator
                  name="de.urszeidler.shr5.ecp.separator3"
                  visible="true">
            </separator>
            <command
                  commandId="de.urszeidler.shr5.ecp.commands.openObjectCommand"
                  label="%open.Generator.Object.command"
                  style="push">
               <parameter
                     name="de.urszeidler.shr5.ecp.commands.openObjectCommand.parameter.type"
                     value="1">
               </parameter>
            </command>
           <command
                 commandId="de.urszeidler.shr5.ecp.commands.openObjectCommand"
                 label="%open.ManagedCharacter.Object.command"
                 style="push">
               <parameter
                     name="de.urszeidler.shr5.ecp.commands.openObjectCommand.parameter.type"
                     value="2">
               </parameter>
            </command>
         </menu>
         <menu
               id="de.urszeidler.shr5.ecp.menus.extraMenu"
               label="%extra.menu.label">
            <command
                  commandId="de.urszeidler.shr5.ecp.commands.createItemWizard"
                  style="push">
            </command>
            <separator
                  name="de.urszeidler.shr5.ecp.separator1"
                  visible="true">
            </separator>
            <command
                  commandId="de.urszeidler.shr5.ecp.commands.openObjectCommand"
                  label="%open.Any.Object.command"
                  style="push">
               <parameter
                     name="de.urszeidler.shr5.ecp.commands.openObjectCommand.parameter.type"
                     value="3">
               </parameter>
            </command>
           <command
                  commandId="de.urszeidler.shr5.ecp.commands.openObjectCommand"
                  label="%open.Item.Object.command"
                  style="push">
               <parameter
                     name="de.urszeidler.shr5.ecp.commands.openObjectCommand.parameter.type"
                     value="4">
               </parameter>
            </command>
            <separator
                  name="de.urszeidler.shr5.ecp.separator2"
                  visible="true">
            </separator>
                        <command
                              commandId="org.eclipse.ui.file.import"
                              label="%command.import"
                              style="push"
                              tooltip="%command.import.description">
            </command>
            
         </menu>
