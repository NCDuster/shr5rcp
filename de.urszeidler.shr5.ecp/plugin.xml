 </extension>
  <extension
        point="org.eclipse.ui.commands">
     <category
           description="%openCharacterWizard.category.description"
           id="de.urszeidler.shr5.ecp.commands.openWizard"
           name="%openCharacterWizard.category.name">
     </category>
     <command
           categoryId="de.urszeidler.shr5.ecp.commands.openWizard"
           description="%openCharacterWizard.command.description"
           id="de.urszeidler.shr5.ecp.commands.openCharacterWizardCommand"
           name="%openCharacterWizard.command.name">
     </command>
     <command
           categoryId="de.urszeidler.shr5.ecp.commands.openWizard"
           id="de.urszeidler.shr5.ecp.commands.openObjectCommand"
           name="%open.object.command.name">
        <commandParameter
              id="de.urszeidler.shr5.ecp.commands.openObjectCommand.parameter.type"
              name="type"
              optional="true">
        </commandParameter>
     </command>
     <command
           id="de.urszeidler.shr5.ecp.commands.createItemWizard"
           name="%create.object.command.name">
     </command>
  </extension>
  <extension
        point="org.eclipse.ui.handlers">
     <handler
           class="de.urszeidler.shr5.ecp.handlers.Shr5CommandHandler"
           commandId="de.urszeidler.shr5.ecp.commands.openCharacterWizardCommand">
     </handler>
     <handler
           class="de.urszeidler.shr5.ecp.handlers.OpenObjectHandler"
           commandId="de.urszeidler.shr5.ecp.commands.openObjectCommand">
     </handler>
     <handler
           class="de.urszeidler.shr5.ecp.handlers.CreateNewObjectsWizardCommandHandler"
           commandId="de.urszeidler.shr5.ecp.commands.createItemWizard">
     </handler>
  </extension>


