   <extension
         point="org.eclipse.emf.ecp.core.filters">
      <FilterProvider
            class="de.urszeidler.shr5.ecp.filter.BasicFilter">
      </FilterProvider>
   </extension>

   <extension
         point="org.eclipse.emf.ecp.ui.modelElementOpener">
      <modelElementOpener     
            class="de.urszeidler.shr5.ecp.opener.ECPAttributModifikatorWertOpener">
         <staticTester
               modelElement="de.urszeidler.eclipse.shr5.FertigkeitsGruppe"
               priority="2">
         </staticTester>
      </modelElementOpener>
      <modelElementOpener     
            class="de.urszeidler.shr5.ecp.opener.ECPAttributModifikatorWertOpener">
         <staticTester
               modelElement="de.urszeidler.eclipse.shr5.Gegenstand"
               priority="2">
         </staticTester>
      </modelElementOpener>
      <modelElementOpener     
            class="de.urszeidler.shr5.ecp.opener.ECPAttributModifikatorWertOpener">
         <staticTester
               modelElement="de.urszeidler.eclipse.shr5.Kleidung"
               priority="2">
         </staticTester>
      </modelElementOpener>
      <modelElementOpener
            class="de.urszeidler.shr5.ecp.opener.ECPAttributModifikatorWertOpener">
         <dynamicTester
               tester="de.urszeidler.shr5.ecp.opener.ECPAttributModifikatorWertOpener">
         </dynamicTester>
      </modelElementOpener>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.urszeidler.shr5.ecp.editor.ShadowrunEditor"
            default="false"
            id="de.urszeidler.eclipse.shadowrun.presentation.editors.ShadowrunEditorID"
            name="%editor.name">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="de.urszeidler.shr5.ecp.Shr5Initalizer">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="de.urszeidler.shr5.ecp.preferences.PrintPreferencePage"
            id="de.urszeidler.shr5.ecp.preferences.PrintPreferencePage"
            name="%print.preferences.page.name">
      </page>
      <page
            class="de.urszeidler.shr5.ecp.preferences.Sr5MainPreferences"
            id="de.urszeidler.shr5.ecp.preferences.basic.page"
            name="%main.preferences.page.name">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="de.urszeidler.shr5.ecp.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="de.urszeidler.shr5.ecp.category.views"
            class="de.urszeidler.shr5.ecp.views.GeneratorValidationView"
            icon="images/valadapter_obj.gif"
            id="de.urszeidler.shr5.ecp.GeneratorValidationView"
            name="%generatorValidation.view.name">
      </view>
      <category
            id="de.urszeidler.shr5.ecp.category.views"
            name="%shadowrun.views.category">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.services">
      <serviceFactory
            factoryClass="de.urszeidler.shr5.ecp.service.UIServiceFactory">
         <service
               serviceClass="de.urszeidler.shr5.ecp.service.ValidationService">
         </service>
      </serviceFactory>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="de.urszeidler.shr5.ecp.character"
            name="%characterGenerator.category.name">
      </category>
      <wizard
            canFinishEarly="false"
            category="de.urszeidler.shr5.ecp.character"
            class="de.urszeidler.shr5.ecp.wizards.NewShr5GeneratorWizard"
            descriptionImage="images/CoreGenerator64.png"
            hasPages="true"
            icon="images/CoreGenerator16.gif"
            id="de.urszeidler.shr5.ecp.wizards.NewShr5GeneratorWizard"
            name="%corerule.wizard.name">
         <description>
            %corerule.wizard.description
         </description>
      </wizard>
      <wizard
            category="de.urszeidler.shr5.ecp.character"
            class="de.urszeidler.shr5.ecp.wizards.NewShr5KarmaGeneratorWizard"
            descriptionImage="images/KarmaGenerator64.png"
            icon="images/KarmaGenerator16.gif"
            id="de.urszeidler.shr5.ecp.wizards.NewShr5KarmaGeneratorWizard"
            name="%karma.wizard.name">
         <description>
            %karma.wizard.description
         </description>
      </wizard>
      <wizard
            category="de.urszeidler.shr5.ecp.character"
            class="de.urszeidler.shr5.ecp.wizards.NewFreeStyleGeneratorWizard"
            descriptionImage="images/FreestyleGenerator64.png"
            icon="images/FreestyleGenerator16.gif"
            id="de.urszeidler.shr5.ecp.wizards.NewFreeStyleGeneratorWizard"
            name="%freestyle.wizard.name">
         <description>
            %freestyle.wizard.description
         </description>
      </wizard>
   </extension>

  <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=project.menu.id">
         <menu
               id="de.urszeidler.shr5.ecp.menus.characterMenu"
               label="%character.menu.label"
               mnemonic="M">
            <command
                  commandId="de.urszeidler.shr5.ecp.commands.openCharacterWizardCommand"
                  mnemonic="S">
            </command>
            <command
                  commandId="de.urszeidler.shr5.ecp.commands.openObjectCommand"
                  label="%open.Generator.Object.command"
                  style="push">
               <parameter
                     name="de.urszeidler.shr5.ecp.commands.openObjectCommand.parameter.type"
                     value="1">
               </parameter>
            </command>
           <command
                 commandId="de.urszeidler.shr5.ecp.commands.openObjectCommand"
                 label="%open.ManagedCharacter.Object.command"
                 style="push">
               <parameter
                     name="de.urszeidler.shr5.ecp.commands.openObjectCommand.parameter.type"
                     value="2">
               </parameter>
            </command>
         </menu>
      </menuContribution>


 </extension>
  <extension
        point="org.eclipse.ui.commands">
     <category
           id="de.urszeidler.shr5.ecp.commands.openWizard"
           name="%openCharacterWizard.category.name">
     </category>
     <command
           categoryId="de.urszeidler.shr5.ecp.commands.openWizard"
           id="de.urszeidler.shr5.ecp.commands.openCharacterWizardCommand"
           name="%openCharacterWizard.command.name">
     </command>
     <command
           categoryId="de.urszeidler.shr5.ecp.commands.openWizard"
           id="de.urszeidler.shr5.ecp.commands.openObjectCommand"
           name="open object">
        <commandParameter
              id="de.urszeidler.shr5.ecp.commands.openObjectCommand.parameter.type"
              name="type"
              optional="true">
        </commandParameter>
     </command>
  </extension>
  <extension
        point="org.eclipse.ui.handlers">
     <handler
           class="de.urszeidler.shr5.ecp.handlers.Shr5CommandHandler"
           commandId="de.urszeidler.shr5.ecp.commands.openCharacterWizardCommand">
     </handler>
     <handler
           class="de.urszeidler.shr5.ecp.handlers.OpenObjectHandler"
           commandId="de.urszeidler.shr5.ecp.commands.openObjectCommand">
     </handler>
  </extension>


</plugin>
