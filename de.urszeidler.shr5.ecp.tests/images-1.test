get-menu "Charakter/Öffne Managed Charakter" | click
with [get-window "Managed Charakter öffnen."] {
    get-tree | select "SC Ki Adept Gunter von Kintzel"
    get-button OK | click
}
get-editor "SC Ki Adept Gunter von Kintzel" | get-label "SC Ki Adept Gunter von Kintzel" | get-property caption 
    | equals "SC Ki Adept Gunter von Kintzel" | verify-true
 
    
get-view "Model Explorer" | get-tree 
    | select "shr5Resources/All/Spielleiter my game stuff/Charakter Gruppe Connections" | double-click
 
 get-view "Model Explorer" |take-screenshot "test" 
 get-eclipse-window | get-object | take-screenshot "test1" 
 get-editor "SC Ki Adept Gunter von Kintzel" |get-object | take-screenshot "test2" 
    
get-tab-folder -index 2 | get-tab-item "SC Ki Adept Gunter von Kintzel" | click
take-screenshot "test3" 

get-editor "SC Ki Adept Gunter von Kintzel" | get-tab-folder | get-tab-item Persona | click
take-screenshot "test4"
with [get-editor "SC Ki Adept Gunter von Kintzel"] {
    with [get-tab-folder] {
        get-tab-item Charakter | click
        take-screenshot "Character" 
        get-tab-item Diary | click
        take-screenshot "Diary" 
        get-tab-item Steigerungen | click
        take-screenshot "Steigerungen" 
        get-tab-item "Character sheet" | click
        take-screenshot "Character sheet" 
        get-tab-item "Shr5 generator" | click
        take-screenshot "Shr5 generator" 
    }
}
